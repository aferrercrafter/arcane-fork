{
  "multipart": [
    {   "when": { "up": "true" },
      "apply": { "model": "arcane:block/arcane_stone_wall_post", "uvlock": true }
    },
    {   "when": { "north": "low" },
      "apply": { "model": "arcane:block/arcane_stone_wall_side", "uvlock": true }
    },
    {   "when": { "east": "low" },
      "apply": { "model": "arcane:block/arcane_stone_wall_side", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "low" },
      "apply": { "model": "arcane:block/arcane_stone_wall_side", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "low" },
      "apply": { "model": "arcane:block/arcane_stone_wall_side", "y": 270, "uvlock": true }
    },
    {   "when": { "north": "tall" },
      "apply": { "model": "arcane:block/arcane_stone_wall_post", "uvlock": true }
    },
    {   "when": { "east": "tall" },
      "apply": { "model": "arcane:block/arcane_stone_wall_post", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "tall" },
      "apply": { "model": "arcane:block/arcane_stone_wall_post", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "tall" },
      "apply": { "model": "arcane:block/arcane_stone_wall_post", "y": 270, "uvlock": true }
    }
  ]
}